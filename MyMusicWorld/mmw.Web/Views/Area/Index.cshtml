@{
    Layout = "/Views/Shared/_Layout_admin.cshtml";
}
@model IEnumerable<mmw.Web.Models.Area>

@{
    ViewBag.Title = "AreaList";
}

<table class="table table-striped">
    <tr><th>ID</th><th>名称</th><th>操作</th></tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.AreaId
            </td>
            <td>@item.AreaName</td>
            <td>@Html.ActionLink("Delete", "Delete", "Area", new { id = item.AreaId }, null)</td>

        </tr>
    }
</table>
<form method="post" action="Area/Create">
    <div>
        区域名称:<input type="text" name="AreaName" />
    </div>
    <div>
        <input type="submit" class="btn btn-primary" value="提交" />
        <input type="button" class="btn btn-sm btn-warning" onclick="location.href=''" value="取消"/>
    </div>
</form>


