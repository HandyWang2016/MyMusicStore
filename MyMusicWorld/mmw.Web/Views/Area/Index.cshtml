@{
    Layout = null;
}
@model IEnumerable<mmw.Web.Models.Area>

@{
    ViewBag.Title = "AreaList";
}

<table>
    <tr><th>ID</th><th>名称</th><th>操作</th></tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.AreaId
            </td>
            <td>@item.AreaName</td>
            <td>@Html.ActionLink("Delete", "Delete", "Area", new { id = item.AreaId }, null)</td>

        </tr>
    }
</table>
<form method="post" action="Area/Create">
    <div>
        区域名称:<input type="text" name="AreaName" />
    </div>
    <div>
        <input type="submit" />
    </div>
</form>


